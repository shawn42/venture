<div class="row">
  <div class="col-sm-12">
    <ul>
      {{#each characters as |char|}}
        {{#link-to 'authenticated.play' char}}
          <li class="btn btn-default" >{{ char.name }} {{ char.level }}</li>
        {{/link-to}}
      {{/each}}
      {{#link-to 'authenticated.add-a-character'}}
        <li class="btn btn-default">New</li>
      {{/link-to}}
    </ul>
  </div>
</div>

